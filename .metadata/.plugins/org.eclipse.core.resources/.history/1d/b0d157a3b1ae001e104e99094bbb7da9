syntax = "proto3";
option java_multiple_files = true;
option java_package = "com.grpc.user.product";

message Category{
	string categoryId = 1;

	string categoryName = 2;

	string imagePath = 3;
}

message Product{

	string productId = 1;

	Category category = 2;

	string productName = 3;

	string productPrice = 4;

	string productDescription = 5;

	int32 viewCount = 6;

	string imagePath = 7;

	int32 rank = 8;

	string userId = 9;

	string totalProducts = 10;
}
